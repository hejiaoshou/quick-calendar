{
  "package": "com.hedingheng.qrl",
  "name": "轻日历",
  "versionName": "1.1.1",
  "versionCode": "3",
  "minPlatformVersion": "1040",
  "icon": "/Common/logo.png",
  "features": [
    { "name": "system.storage"},
    { "name": "system.network"},
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.device"},
    {"name": "system.fetch"},
    { "name": "system.geolocation" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug",
    "data":{
      "umeng_appkey" : "马赛克"
    }
  },
  "router": {
    "entry": "home",
    "pages": {
      "home": {
        "component": "index"
      },
      "yue": {
        "component": "index"
      },
      "year": {
        "component": "index"
      },
      "news": {
        "component": "news"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#e1c8a0",
    "titleBarTextColor": "#fff",
    "titleBar": false,
    "menu": false,
    "pages": {
      "news": {
        "titleBar": true
      }
    }
  }
}